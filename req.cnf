[ req ]
distinguished_name = distinguished_name
default_md = sha512
prompt = no
req_extensions = extensions

[ distinguished_name ]
countryName = US
stateOrProvinceName = New York
localityName = Brooklyn
emailAddress = info@test.com
commonName = localhost

[ extensions ]
subjectAltName = @alternate_names

[ alternate_names ]
IP.1 = 127.0.0.1
DNS.1 = localhost
DNS.2 = *.localhost
